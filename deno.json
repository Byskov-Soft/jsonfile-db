{
  "name": "@bysk/jsonfile-db",
  "version": "0.1.1",
  "license": "MIT",
  "nodeModulesDir": "none",
  "tasks": {
    "test": "deno test --allow-read --allow-write test/",
    "publish": "npx jsr publish --allow-slow-types",
    "publish:same": "npx jsr publish --allow-slow-types --no-version-bump"
  },
  "lint": {
    "rules": {
      "exclude": ["no-slow-types"]
    }
  },
  "fmt": {
    "singleQuote": true,
    "indentWidth": 2,
    "lineWidth": 100,
    "semiColons": false
  },
  "exports": {
    ".": "./mod.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert",
    "@std/expect": "jsr:@std/expect",
    "@std/fs": "jsr:@std/fs",
    "date-fns": "npm:date-fns@3.0.6",
    "uuid": "npm:uuid@9.0.0"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "deno.json",
      "LICENSE-locally-db",
      "mod.ts",
      "README.md"
    ],
    "exclude": [
        ".gitignore",
        "dist",
        "test"
    ]
  }
}
